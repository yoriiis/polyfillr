{
	"name": "polyfillr",
	"version": "2.0.0",
	"description": "Polyfillr dynamically load polyfills with dynamic import before start your application",
	"keywords": [
		"polyfill-loader",
		"polyfill",
		"dynamic-import"
	],
	"homepage": "https://github.com/yoriiis/polyfillr",
	"bugs": "https://github.com/yoriiis/polyfillr/issues",
	"repository": "https://github.com/yoriiis/polyfillr.git",
	"license": "MIT",
	"author": "Yoriiis aka Joris DANIEL",
	"main": "src/index.js",
	"scripts": {
		"build:example": "rm -rf ./example/dist/ && webpack --config=./example/webpack.config.js --mode=production",
		"start:example": "rm -rf ./example/dist/ && webpack --config=./example/webpack.config.js --mode=development",
		"test:eslint": "eslint -c ./.eslintrc.js .",
		"test:markdown": "markdownlint '**/*.md' --ignore node_modules"
	},
	"dependencies": {
		"promise-polyfill": "^8.1.3"
	},
	"devDependencies": {
		"@babel/core": "7.8.3",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/preset-env": "7.8.3",
		"babel-eslint": "10.0.3",
		"babel-loader": "8.0.6",
		"eslint": "6.8.0",
		"eslint-config-standard": "14.1.0",
		"eslint-plugin-import": "2.20.0",
		"eslint-plugin-node": "11.0.0",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-standard": "4.0.1",
		"markdownlint-cli": "0.21.0",
		"prettier-eslint": "9.0.1",
		"webpack": "4.41.5",
		"webpack-cli": "3.3.12"
	},
	"engines": {
		"node": ">=8.11.2"
	}
}
